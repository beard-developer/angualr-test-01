{
  "name": "angualr-test-01",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=localhost --port=4200 --open",
    "build": "ng build",
    "build:prod": "ng build --configuration production --aot",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:html": "htmlhint --config .htmlhintrc src/app/**/*.html",
    "lint:styles": "stylelint src/app/**/*.scss",
    "format:prettier": "pretty-quick --staged",
    "format:all": "run-s lint:styles lint:fix lint:html format:prettier",
    "test": "ng test --code-coverage",
    "test:once": "ng test --watch=false",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadlessCI --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.4",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
}
